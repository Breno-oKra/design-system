{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
    "build": {
        "outputs": [ 
            "dist/**",
            "storybook-statics/**"
        ],
        "dependsOn": [
            "^build"
        ]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}

